{
  "extends": "airbnb-base",
  "rules": {
    "indent": ["warn", 4, {"SwitchCase": 1}],
    "arrow-parens": "off",
    "quotes": ["error", "single"],
    "space-before-function-paren": "off",
    "comma-dangle": ["off", {
      "arrays": "always-multiline",
      "objects": "always-multiline",
      "exports": "always-multiline",
      "functions": "ignore"
    }],
    "global-require": "off"
  }
}
